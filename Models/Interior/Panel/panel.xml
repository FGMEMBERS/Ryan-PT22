<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>
    
    <!-- Objets transparents -->
  </animation>

  <!-- MAGNETO -->
  <model>
    <path>Aircraft/Ryan-PT22/Models/Interior/Panel/Instruments/magneto/mags.xml</path>
    <offsets>
      <x-m>  0.000 </x-m>
      <y-m> -0.260 </y-m>
      <z-m> -0.120 </z-m>
    </offsets>
  </model>
  
  <!-- VSI -->
  <model>
    <path>Aircraft/Ryan-PT22/Models/Interior/Panel/Instruments/vsi/vsi.xml</path>
    <offsets>
      <x-m>  0.000 </x-m>
      <y-m> -0.100 </y-m>
      <z-m>  0.034 </z-m>
    </offsets>
  </model>
  
