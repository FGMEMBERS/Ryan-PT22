<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>kinner-R5.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>moteurlow</object-name>
  </animation>

  <model>
    <name>Bloc1</name>
    <path>Aircraft/Ryan-PT22/Models/Engine/blocpiston.xml</path>
  </model>

  <model>
    <name>Bloc2</name>
    <path>Aircraft/Ryan-PT22/Models/Engine/blocpiston.xml</path>
    <offsets>
      <roll-deg> 70 </roll-deg>
    </offsets>
  </model>

  <model>
    <name>Bloc3</name>
    <path>Aircraft/Ryan-PT22/Models/Engine/blocpiston.xml</path>
    <offsets>
      <roll-deg> 140 </roll-deg>
    </offsets>
  </model>

  <model>
    <name>Bloc4</name>
    <path>Aircraft/Ryan-PT22/Models/Engine/blocpiston.xml</path>
    <offsets>
      <roll-deg> -70 </roll-deg>
    </offsets>
  </model>

  <model>
    <name>Bloc5</name>
    <path>Aircraft/Ryan-PT22/Models/Engine/blocpiston.xml</path>
    <offsets>
      <roll-deg> -140 </roll-deg>
    </offsets>
  </model>

  <animation>
    <name>HighPoly</name>
    <object-name>Bloc1</object-name>
    <object-name>Bloc2</object-name>
    <object-name>Bloc3</object-name>
    <object-name>Bloc4</object-name>
    <object-name>Bloc5</object-name>
  </animation>

  <animation>
    <name>LowPoly</name>
    <object-name>moteurlow</object-name>
  </animation>

  <animation>
    <type>range</type>
    <object-name>HighPoly</object-name>
    <min-m>0</min-m>
    <max-property>/sim/rendering/static-lod/detailed</max-property>
  </animation>

  <animation>
    <type>range</type>
    <object-name>LowPoly</object-name>
    <min-property>/sim/rendering/static-lod/detailed</min-property>
    <max-property>/sim/rendering/static-lod/bare</max-property>
  </animation>

</PropertyList>
